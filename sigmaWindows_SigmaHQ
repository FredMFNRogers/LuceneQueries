./sigma/rules/windows/process_creation/proc_creation_win_sysinternals_pssuspend_susp_execution.yml
(process.pe.original_file_name:pssuspend.exe OR (process.executable:(*\\pssuspend.exe OR *\\pssuspend64.exe))) AND process.command_line:*msmpeng.exe*
 
./sigma/rules/windows/process_creation/proc_creation_win_hktl_invoke_obfuscation_via_use_mhsta.yml
process.command_line:*set* AND process.command_line:*\&\&* AND process.command_line:*mshta* AND process.command_line:*vbscript\:createobject* AND process.command_line:*.run* AND process.command_line:*\(window.close\)*
 
./sigma/rules/windows/process_creation/proc_creation_win_powershell_susp_ps_appdata.yml
(process.command_line:(*powershell.exe* OR *\\powershell* OR *\\pwsh* OR *pwsh.exe*)) AND ((process.command_line:*\/c\ * AND process.command_line:*\\AppData\\*) AND (process.command_line:(*Local\\* OR *Roaming\\*)))
 
./sigma/rules/windows/process_creation/proc_creation_win_installutil_download.yml
(process.executable:*\\InstallUtil.exe OR process.pe.original_file_name:InstallUtil.exe) AND (process.command_line:(*ftp\:\/\/* OR *http\:\/\/* OR *https\:\/\/*))
 
./sigma/rules/windows/process_creation/proc_creation_win_renamed_megasync.yml
process.pe.original_file_name:megasync.exe AND (NOT process.executable:*\\megasync.exe)
 
./sigma/rules/windows/process_creation/proc_creation_win_hktl_covenant.yml
((process.command_line:*\-Sta* AND process.command_line:*\-Nop* AND process.command_line:*\-Window* AND process.command_line:*Hidden*) AND (process.command_line:(*\-Command* OR *\-EncodedCommand*))) OR (process.command_line:(*sv\ o\ \(New\-Object\ IO.MemorySteam\);sv\ d\ * OR *mshta\ file.hta* OR *GruntHTTP* OR *\-EncodedCommand\ cwB2ACAAbwAgA*))
 
./sigma/rules/windows/process_creation/proc_creation_win_hktl_zipexec.yml
(process.command_line:*\/generic\:Microsoft_Windows_Shell_ZipFolder\:filename\=* AND process.command_line:*.zip* AND process.command_line:*\/pass\:* AND process.command_line:*\/user\:*) OR (process.command_line:*\/delete* AND process.command_line:*Microsoft_Windows_Shell_ZipFolder\:filename\=* AND process.command_line:*.zip*)
 
./sigma/rules/windows/process_creation/proc_creation_win_cmd_mklink_osk_cmd.yml
(process.executable:*\\cmd.exe OR process.pe.original_file_name:Cmd.Exe) AND (process.command_line:*mklink* AND process.command_line:*\\osk.exe* AND process.command_line:*\\cmd.exe*)
 
./sigma/rules/windows/process_creation/proc_creation_win_cmd_rmdir_execution.yml
